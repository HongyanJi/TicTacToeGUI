<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" style="-fx-background-color: Cornsilk;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.uiowa.TempController">
      <Label fx:id="winner" alignment="CENTER" layoutX="20" layoutY="20.0" prefHeight="50.0" prefWidth="260.0" style="-fx-background-color: BlanchedAlmond;" text="lable">
      <font>
         <Font name="Arial Bold" size="24.0" />
      </font></Label>
      <Button fx:id="regame" layoutX="60.0" layoutY="100.0" mnemonicParsing="false" onAction="#regame" prefHeight="25.0" prefWidth="180.0" style="-fx-background-color: LawnGreen;" text="Two human? ">
      <font>
         <Font name="Arial Bold" size="18.0" />
      </font></Button>
      <Button fx:id="close" layoutX="100.0" layoutY="220.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0" prefWidth="100.0" style="-fx-background-color: Chartreuse;" text="Close? ">
      <font>
         <Font name="Arial Bold" size="18.0" />
      </font></Button>
   <Button fx:id="regameWithComputer" layoutX="60.0" layoutY="160.0" mnemonicParsing="false" onAction="#regameWithComputer" prefHeight="25.0" prefWidth="180.0" style="-fx-background-color: greenyellow;" text="With computer? ">
      <font>
         <Font name="Arial Bold" size="18.0" />
      </font></Button>


</AnchorPane>
